<!-- navigation panel -->
<nav class="navbar navbar-default navbar-fixed-top nav-transparent overlay-nav sticky-nav nav-border-bottom bg-white shop-nav" role="navigation">
	<div class="container">
		<div class="row">
			<!-- logo -->
			<div class="col-md-2 pull-left"><a class="logo-light" href="index.php"><img alt="" src="catalog/view/theme/default/image/logo-light.png" class="logo" /></a><a class="logo-dark" href="index.php"><img alt="" src="catalog/view/theme/default/image/logo-light.png" class="logo" /></a></div>
			<!-- end logo -->
			<!-- search and cart  -->
			<div class="col-md-2 no-padding-left search-cart-header pull-right">
				<div id="top-search">
					<!-- nav search -->
					<a href="#search-header" class="header-search-form"><i class="fa fa-search search-button"></i></a>
					<!-- end nav search -->
				</div>
				<!-- search input-->
				<form id="search-header" method="post" action="#" name="search-header" class="mfp-hide search-form-result">
					<div class="search-form position-relative">
						<button type="submit" class="fa fa-search close-search search-button"></button>
						<input type="text" name="search" class="search-input" placeholder="Enter your keywords..." autocomplete="off">
					</div>
				</form>
				<!-- end search input -->
				<div class="top-cart">
					<!-- nav shopping bag -->
					<a href="#" class="shopping-cart">
						<i class="fa fa-shopping-cart"></i>
						<div class="subtitle">(1) Items</div>
					</a>
					<!-- end nav shopping bag -->
					<!-- shopping bag content -->
					<div class="cart-content">
						<ul class="cart-list">
							<li>
								<a title="Remove item" class="remove" href="#">×</a>
								<a href="#">
									<img width="90" height="90" alt="" src="http://placehold.it/90x90">Leather Craft
								</a>
								<span class="quantity">1 × <span class="amount">$160</span></span>
								<a href="#">Edit</a>
							</li>
						</ul>
						<p class="total">Subtotal: <span class="amount">$160</span></p>
						<p class="buttons">
							<a href="{{ shopping_cart }}" title="{{ text_shopping_cart }}" class="btn btn-very-small-white no-margin-bottom margin-seven pull-left no-margin-lr">{{ text_shopping_cart }}</a>
							<a href="{{ checkout }}" title="{{ text_checkout }}"  class="btn btn-very-small-white no-margin-bottom margin-seven no-margin-right pull-right">{{ text_checkout }}</a>
						</p>
					</div>
					<!-- end shopping bag content -->
				</div>
			</div>
			<!-- end search and cart  -->
			<!-- toggle navigation -->
			<div class="navbar-header col-sm-8 col-xs-2 pull-right">
